[Unit]
Description=AOS Update Manager
Wants=network-online.target aos-communicationmanager.service
After=network-online.target aos-communicationmanager.service
PartOf=aos.target

[Service]
ExecStart=/usr/bin/aos_updatemanager -c /etc/aos/aos_updatemanager.cfg -v debug -j
Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target